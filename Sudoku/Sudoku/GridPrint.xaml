                <TextBox Grid.Row="4" Grid.Column="8" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>

                <TextBox Grid.Row="5" Grid.Column="0" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>
                <TextBox Grid.Row="5" Grid.Column="1" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>
                <TextBox Grid.Row="5" Grid.Column="2" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>
                <TextBox Grid.Row="5" Grid.Column="3" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>
                <TextBox Grid.Row="5" Grid.Column="4" TextChanged="TextBox_TextChanged" PreviewTextInput="TextBox_PreviewTextInput"></TextBox>
